{
  "name": "fusion-project",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run dev:backend\" \"pnpm run dev:frontend\"",
    "dev:frontend": "cd frontend && pnpm run dev",
    "dev:backend": "cd backend && pnpm run dev",
    "build": "pnpm run build:frontend && pnpm run build:backend",
    "build:frontend": "cd frontend && pnpm run build",
    "build:backend": "cd backend && pnpm run build",
    "start:backend": "cd backend && pnpm start",
    "install:all": "pnpm install && cd frontend && pnpm install && cd ../backend && pnpm install",
    "typecheck": "pnpm run typecheck:frontend && pnpm run typecheck:backend",
    "typecheck:frontend": "cd frontend && pnpm run typecheck",
    "typecheck:backend": "cd backend && pnpm run typecheck"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
