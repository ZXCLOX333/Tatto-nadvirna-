#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/admin/Desktop/Нова папка (4)/node_modules/.pnpm/vite@7.1.3_@types+node@24.3_5cbe8aa65cbeb0c28d4c4aa08014fec3/node_modules/vite/bin/node_modules:/mnt/c/Users/admin/Desktop/Нова папка (4)/node_modules/.pnpm/vite@7.1.3_@types+node@24.3_5cbe8aa65cbeb0c28d4c4aa08014fec3/node_modules/vite/node_modules:/mnt/c/Users/admin/Desktop/Нова папка (4)/node_modules/.pnpm/vite@7.1.3_@types+node@24.3_5cbe8aa65cbeb0c28d4c4aa08014fec3/node_modules:/mnt/c/Users/admin/Desktop/Нова папка (4)/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/admin/Desktop/Нова папка (4)/node_modules/.pnpm/vite@7.1.3_@types+node@24.3_5cbe8aa65cbeb0c28d4c4aa08014fec3/node_modules/vite/bin/node_modules:/mnt/c/Users/admin/Desktop/Нова папка (4)/node_modules/.pnpm/vite@7.1.3_@types+node@24.3_5cbe8aa65cbeb0c28d4c4aa08014fec3/node_modules/vite/node_modules:/mnt/c/Users/admin/Desktop/Нова папка (4)/node_modules/.pnpm/vite@7.1.3_@types+node@24.3_5cbe8aa65cbeb0c28d4c4aa08014fec3/node_modules:/mnt/c/Users/admin/Desktop/Нова папка (4)/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@7.1.3_@types+node@24.3_5cbe8aa65cbeb0c28d4c4aa08014fec3/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@7.1.3_@types+node@24.3_5cbe8aa65cbeb0c28d4c4aa08014fec3/node_modules/vite/bin/vite.js" "$@"
fi
